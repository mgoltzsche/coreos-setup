{
	"environment": {
		"NAME": "world"
	},
	"services": {
		"owncloud": {
			"image": "docker://owncloud",
			"environment": {
				"VAL1": "1st",
				"VAL2": "cccc"
			},
			"http": {
				"port": 80,
				"protect": false,
				"subdomain": ""
			},
			"checks": [
				{"port": 80},
				{"url": "http://127.0.0.1/health"},
				{"run": "nc..."}
			]
		},
		"hello": {
			"image": "docker://alpine",
			"exec": [ "/usr/bin/nc", "-lk", "0.0.0.0", "8080", "-e", "echo", "Hello", "$NAME" ],
			"environment": {
				"NAME": "world overwritten",
				"VAL2": "cccc"
			},
			"http": {
				"port": 8080,
				"protect": true
			}
		}
	}
}
